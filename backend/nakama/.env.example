# Nakama Environment Variables
# Copy this file to .env and update with your own values for production

# Database Configuration
POSTGRES_DB=nakama
POSTGRES_USER=nakama
POSTGRES_PASSWORD=your_secure_password_here

# Nakama Server Keys
# IMPORTANT: Change these for production deployments!
NAKAMA_CONSOLE_HTTP_KEY=your_console_key_here
NAKAMA_SESSION_ENCRYPTION_KEY=your_32_char_encryption_key_here
NAKAMA_REFRESH_ENCRYPTION_KEY=your_32_char_refresh_key_here
NAKAMA_SERVER_KEY=your_server_key_here
NAKAMA_RUNTIME_HTTP_KEY=your_runtime_key_here

# Testing Mode (set to false in production)
TESTING_MODE=false
